{
    "name": "extension",
    "displayName": "Extensaion",
    "description": "descriprion",
    "version": "0.0.1",
    "publisher": "stewart_robertson@sky.com",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:textTools.insertLineAbove"
    ],
    "contributes": {
        "commands": [
            {
                "title": "Insert Line Above",
                "command": "textTools.insertLineAbove"
            }
        ]
    },
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc-watch -p ./",
        "watch-tests" : "tsc-watch --onSuccess 'npm run runtests' -p ./ ",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "runtests": "mocha out/test/**/*.js",
        "test": "npm run compile && npm run runtests"
    },
    "devDependencies": {
        "@types/chai": "^4.1.2",
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "chai": "^4.1.2",
        "tsc-watch": "^1.0.16",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {}
}
